# Apple Bot System 环境配置文件
# 复制此文件为 .env 并修改相应配置

# Flask配置
FLASK_CONFIG=development
SECRET_KEY=apple-bot-secret-key-2024
DEBUG=True

# 任务配置
MAX_CONCURRENT_TASKS=3
TASK_TIMEOUT=1800

# Redis配置 (可选)
REDIS_URL=redis://localhost:6379/0

# IP代理配置 (可选)
PROXY_ROTATION_ENABLED=False
PROXY_API_URL=

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=app.log

# WebSocket配置
SOCKETIO_ASYNC_MODE=eventlet